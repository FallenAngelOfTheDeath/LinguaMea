<android.support.design.widget.CoordinatorLayout
    android:id="@+id/main_content"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.ninebit.linguamea.fragment.dictionaries.OIDictionaryFragment">
    <FrameLayout
        android:id="@+id/container_custom_dict"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>

    <android.support.v7.widget.RecyclerView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/recycler_view_oi_dictionary"/>

    <android.support.design.widget.FloatingActionButton
        app:layout_behavior="com.example.ninebit.linguamea.custombehavior.ShrinkBehavior"
        app:srcCompat="@drawable/ic_add"
        android:id="@+id/fab_add"
        android:layout_gravity="bottom|right"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
</android.support.design.widget.CoordinatorLayout>